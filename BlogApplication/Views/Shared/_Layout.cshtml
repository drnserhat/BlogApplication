							<a asp-controller="Home" asp-action="Index"><img src="~/assets/images/logo.png" alt=""></a>
						</div>
						<!-- Phone Menu button -->
						<button id="menu" class="menu hidden-md-up"></button>
					</div>
					<div class="col-md-9 nav-bg d-flex align-items-center">
						<nav class="navigation">
							<ul>
								<li>
									<a asp-controller="Home" asp-action="Index">Ana Sayfa</a><i class="ion-ios-plus-empty hidden-md-up"></i>
							
