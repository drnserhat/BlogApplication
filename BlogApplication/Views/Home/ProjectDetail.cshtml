@model Hizmetler
<section class="inner-intro bg-img light-color overlay-before parallax-background">
	<div class="container">
		<div class="row title">
			<div class="title_row">
				<h1 data-title="Project Details"><span>Proje Detay</span></h1>
				<div class="page-breadcrumb">
					<a>Home</a>/ <span>Projeet Detay</span>
				</div>

			</div>

		</div>
	</div>
</section>
<div id="project-detail-section" class="padding pb-60 pt-xs-60">
	<div class="container">
		<div class="row ">
			<div class="col-sm-12">
				<div class="heading-box pb-30 ">
					<h2>@Model.HizmetBaslik</h2>
					<span class="b-line l-left"></span>
				</div>

			</div>
		</div>
		<div class="row">
			<div class="col-sm-12">
				<div class="project-details">
					<figure>
						<img src="~/@Model.FotoUrl" alt="">
					</figure>

				</div>
			</div>
			<div class="col-sm-12 col-lg-12 mt-30">
				<div class="box-title mb-20">
					<h3>@Model.HizmetAltBaslik</h3>
				</div>
				<div class="text-content">
					@Html.Raw(Model.HizmetDetay)

				</div>
			</div>



		</div>
	</div>
</div>
